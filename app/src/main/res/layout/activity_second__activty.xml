<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ross.firstactivity.Second_Activity">



    <TextView
    android:id="@+id/RA"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Your RA is "
    android:layout_marginTop="250dp"
    android:layout_margin="100dp"
    />
    <TextView
        android:id="@+id/FG"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your FG is "
        android:layout_marginTop="41dp"
        android:layout_alignTop="@+id/RA"
        android:layout_alignRight="@+id/RA"
        android:layout_alignEnd="@+id/RA" />

    <Button
        android:id="@+id/ret"
        android:layout_width="100dp"
        android:layout_height="52dp"
        android:text="Return"
        tools:layout_editor_absoluteY="406dp"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="200dp"
        android:layout_centerInParent="true"
        android:onClick="returnActivity"
        />
</RelativeLayout>
